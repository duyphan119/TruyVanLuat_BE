    <ns1:MaViPham>diem_37_4_l</ns1:MaViPham>
    <ns1:TuKhoa></ns1:TuKhoa>
    <ns1:GhiChu></ns1:GhiChu>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.semanticweb.org/duyphan/ontologies/2023/5/trafficlaws#DiemCKhoan2Dieu11NghiDdinh100">
